{
  "expo": {
    "name": "U-Pick",
    "slug": "U-Pick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/u-pick-logo.png",
    "splash": {
      "image": "./assets/img/splash-screen/ios/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/u-pick-logo.png",
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
